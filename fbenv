import { ListViewDataConnection } from '../store/app-settings/app-settings.types';
// Need to refactor the below imports;
import * as WidgetsBarrel from './index';
import { PageSetting } from './pages.types';

export class DataConnections {
  dataConnections: ListViewDataConnection[] = [];
  constructor() {
    let dataConnection1 = new ListViewDataConnection();
    dataConnection1.Id = 1;
    dataConnection1.Title = 'All Items';
    dataConnection1.properties = {
      'list': {
        'RootFolder': { 'Name': 'cciRecords' }, 'Views': [{ 'Id': 'f54a3664-06d5-410b-92aa-0c893df9b95a', 'Title': 'All Items' },
        { 'Id': 'c622163a-9ec0-4514-bb79-63a488345b52', 'Title': 'Folder View' }, { 'Id': '2255ab2f-9b62-43ad-a59f-1e43b90ffe9b', 'Title': 'Recent Records' },
        { 'Id': 'c3cce81d-ea4a-4354-98c9-c935cb3c69ad', 'Title': 'Expiring Soon' }, { 'Id': '331dd06e-f5fb-4955-b5ea-17a6c37d4e29', 'Title': 'My Contracts' }], 'BaseType': 0, 'Title': 'Records'
      }, 'view': { 'Id': '2255ab2f-9b62-43ad-a59f-1e43b90ffe9b', 'Title': 'Recent Records' },
      'siteRelativeUrl': null,
      'viewGUID': null,
    };

    let dataConnection2 = new ListViewDataConnection();
    dataConnection2.Id = 2;
    dataConnection2.Title = 'All Items';
    dataConnection2.properties = {
      'list': {
        'RootFolder': { 'Name': 'cciCounterparties' }, 'Views': [{ 'Id': '61d77758-20a9-407d-af04-447ef1b500da', 'Title': 'All Items' }],
        'BaseType': 0, 'Title': 'Counterparties'
      },
      'view': { 'Id': '61d77758-20a9-407d-af04-447ef1b500da', 'Title': 'All Items' },
      'siteRelativeUrl': null,
      'viewGUID': null
    };

    let rawDataConnections = [];
    rawDataConnections.push(dataConnection1);
    rawDataConnections.push(dataConnection2);

    for (let i = 0; i < rawDataConnections.length; i++) {
      this.dataConnections[i] = rawDataConnections[i];
    }
  }
}
